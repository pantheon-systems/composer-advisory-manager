{
  "name": "pantheon-systems/composer-advisory-manager",
  "description": "Manages Composer security advisories to maintain build continuity while promoting remediation on Pantheon.",
  "type": "composer-plugin",
  "license": "MIT",
  "keywords": [
    "composer",
    "plugin",
    "security",
    "advisory",
    "audit",
    "pantheon",
    "build",
    "continuity"
  ],
  "homepage": "https://github.com/pantheon-systems/composer-advisory-manager",
  "support": {
    "issues": "https://github.com/pantheon-systems/composer-advisory-manager/issues",
    "source": "https://github.com/pantheon-systems/composer-advisory-manager"
  },
  "authors": [
    {
      "name": "Pantheon Systems",
      "homepage": "https://pantheon.io"
    }
  ],
  "autoload": {
    "psr-4": {
      "Pantheon\\ComposerAdvisoryManager\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "composer-plugin-api": "^2.0"
  },
  "extra": {
    "class": "Pantheon\\ComposerAdvisoryManager\\AdvisoryManagerPlugin"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
